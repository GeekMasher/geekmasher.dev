{
  "version": 2,
  "public": true,
  "github": {
    "enabled": true
  },
  "alias": [
    "geekmasher.dev"
  ],
  "build": {
    "env": {
      "HUGO_VERSION": "0.98.0"
    }
  },
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        },
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store"
        }
      ]
    }
  ]
}
